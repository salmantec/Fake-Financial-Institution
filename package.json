{
  "name": "fake_financial_institution",
  "version": "0.0.1",
  "description": "Internal API for a fake financial institution",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "production": "node dist/models/migrate.js && node dist/server.js",
    "serve": "node dist/models/migrate.js && nodemon --delay 1 dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.44.3",
    "sequelize-typescript": "^0.6.1",
    "shelljs": "^0.8.3",
    "swagger-ui-express": "^4.0.2",
    "umzug": "^2.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.0.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.136",
    "@types/lusca": "^1.6.0",
    "@types/morgan": "^1.7.36",
    "@types/node": "^11.13.4",
    "@types/umzug": "^2.2.2",
    "@types/validator": "^10.11.0",
    "concurrently": "^4.1.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "typescript": "^3.6.4"
  }
}
